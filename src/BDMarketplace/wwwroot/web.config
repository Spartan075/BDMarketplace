<?xml version="1.0" encoding="utf-8"?>
<configuration>
<handlers>
  <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
</handlers>
<rewrite>
  <rules>
    <rule name="Sommarstugan">
      <match url="/*" />
      <action type="Rewrite" url="server.js" />
    </rule>
  </rules>
</rewrite>
</configuration>
     